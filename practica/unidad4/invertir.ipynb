   "source": [
    "numero = int(input(\"Ingrese un número: \"))\n",
    "invertido = 0\n",
    "original = numero\n",
    "\n",
    "while numero > 0:\n",
    "    digito = numero % 10\n",
    "    invertido = invertido * 10 + digito\n",
    "    numero = numero // 10\n",
    "\n",
    "print(f\"El número {original} invertido es: {invertido}\")"
   ]
  }
